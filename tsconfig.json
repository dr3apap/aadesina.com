{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
  "baseUrl": ".",
  "paths": {
      "~/*": ["./src/*"]
    },
  "strictNullChecks":true,
  "noImplicitAny":false,
  "noEmitOnError":true,
  "jsx":"preserve",
    "importsNotUsedAsValues": "remove"
  },
  "include": ["**/*", ".eslintrc.cjs"],
  "exclude": ["node_modules", "dist", ".netlify", "public"]
}


