---
import MainLayout from '../layouts/MainLayout.astro'
import { siteMetaData } from '../config'
import { ROUTES } from '../constants/routes.js'
const { mainPage, mainPageDes } = siteMetaData
const page = {
    route:ROUTES.home.link,
}
---

<MainLayout
  title={mainPage}
  description={mainPageDes}
  ogTitle={mainPage}
  ogGradient='1'
  page={page}
/>

<script>
    import "../scripts/demo.js"
</script>
